		  restrict: 'E',
		  templateUrl: 'lists/featured-review-item.html',
		  controller: 'featuredReviewItemCtrl',
		  scope:{
			  item: "="
		  },
		  link:function(scope,element,attrs){
		  }
	  };
	});
</script>
 
